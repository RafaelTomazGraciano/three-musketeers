#include <stdio.tm>

int main() {
    int i;
    
    printf("Count from 0 to 4:\n");
    i = 0;
    while (i < 5) {
        printf("%d ", i);
        i = i + 1;
    }
    printf("\n");
    
    printf("\nCount from 10 to 6:\n");
    i = 10;
    while (i > 5) {
        printf("%d ", i);
        i = i - 1;
    }
    printf("\n");
    
    printf("\nDouble until exceeds 100:\n");
    i = 1;
    while (i <= 100) {
        printf("%d ", i);
        i = i * 2;
    }
    printf("\n");
    
    printf("\nSum of first 5 numbers:\n");
    i = 1;
    int sum;
    sum = 0;
    while (i <= 5) {
        sum = sum + i;
        i = i + 1;
    }
    printf("Sum = %d\n", sum);
    
    printf("\nSkip condition (starts false):\n");
    i = 10;
    while (i < 5) {
        printf("%d ", i);
        i = i + 1;
    }
    printf("Loop skipped\n");
    
    return 0;
}