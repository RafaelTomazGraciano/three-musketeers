#include <stdio.tm>

int main() {
    int i;
    
    printf("Do-while count 0 to 4:\n");
    i = 0;
    do {
        printf("%d ", i);
        i = i + 1;
    } while (i < 5);
    printf("\n");
    
    printf("\nDo-while executes at least once:\n");
    i = 10;
    do {
        printf("Executed with i = %d\n", i);
        i = i + 1;
    } while (i < 5);
    
    printf("\nDo-while countdown:\n");
    i = 5;
    do {
        printf("%d ", i);
        i = i - 1;
    } while (i > 0);
    printf("\n");
    
    printf("\nDo-while sum:\n");
    i = 1;
    int sum;
    sum = 0;
    do {
        sum = sum + i;
        i = i + 1;
    } while (i <= 5);
    printf("Sum = %d\n", sum);
    
    printf("\nDo-while with break:\n");
    i = 0;
    do {
        printf("%d ", i);
        i = i + 1;
        if (i == 3) {
            break;
        }
    } while (i < 10);
    printf("\n");
    
    return 0;
}