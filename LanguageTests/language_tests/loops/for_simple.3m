#include <stdio.tm>

int main() {
    int i;
    
    printf("Count from 0 to 4:\n");
    for (i = 0; i < 5; i = i + 1) {
        printf("%d ", i);
    }
    printf("\n");
    
    printf("\nCount from 1 to 10:\n");
    for (i = 1; i <= 10; i = i + 1) {
        printf("%d ", i);
    }
    printf("\n");
    
    printf("\nCount backwards from 5 to 1:\n");
    for (i = 5; i > 0; i = i - 1) {
        printf("%d ", i);
    }
    printf("\n");
    
    printf("\nEven numbers from 0 to 10:\n");
    for (i = 0; i <= 10; i = i + 2) {
        printf("%d ", i);
    }
    printf("\n");
    
    printf("\nSum of numbers 1 to 5:\n");
    int sum;
    sum = 0;
    for (i = 1; i <= 5; i = i + 1) {
        sum = sum + i;
    }
    printf("Sum = %d\n", sum);
    
    return 0;
}