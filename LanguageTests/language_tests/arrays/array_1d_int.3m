#include <stdio.tm>

int main() {
    int arr[5];
    int i;
    
    // Initialize array
    arr[0] = 10;
    arr[1] = 20;
    arr[2] = 30;
    arr[3] = 40;
    arr[4] = 50;
    
    // Print array elements
    printf("Array elements:\n");
    for (i = 0; i < 5; i = i + 1) {
        printf("arr[%d] = %d\n", i, arr[i]);
    }
    
    // Calculate sum
    int sum;
    sum = 0;
    for (i = 0; i < 5; i = i + 1) {
        sum = sum + arr[i];
    }
    printf("Sum: %d\n", sum);
    
    // Modify elements
    for (i = 0; i < 5; i = i + 1) {
        arr[i] = arr[i] * 2;
    }
    
    printf("After multiplication by 2:\n");
    for (i = 0; i < 5; i = i + 1) {
        printf("arr[%d] = %d\n", i, arr[i]);
    }
    
    return 0;
}