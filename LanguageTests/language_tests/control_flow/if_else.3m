#include <stdio.tm>

int main() {
    int age;
    age = 18;
    
    printf("Age verification system\n");
    printf("Age: %d\n", age);
    
    if (age >= 18) {
        printf("You are an adult\n");
    } else {
        printf("You are a minor\n");
    }
    
    int score;
    score = 45;
    printf("\nScore: %d\n", score);
    
    if (score >= 50) {
        printf("Passed\n");
    } else {
        printf("Failed\n");
    }
    
    int temperature;
    temperature = 25;
    printf("\nTemperature: %d\n", temperature);
    
    if (temperature > 30) {
        printf("It's hot\n");
    } else {
        printf("It's not hot\n");
    }
    
    return 0;
}