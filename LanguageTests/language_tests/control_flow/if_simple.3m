#include <stdio.tm>

int main() {
    int x;
    x = 10;
    
    printf("Testing simple if statement\n");
    printf("x = %d\n", x);
    
    if (x > 5) {
        printf("x is greater than 5\n");
    }
    
    if (x < 5) {
        printf("x is less than 5\n");
    }
    
    if (x == 10) {
        printf("x is equal to 10\n");
    }
    
    int y;
    y = 3;
    printf("\ny = %d\n", y);
    
    if (y > 5) {
        printf("y is greater than 5\n");
    }
    
    if (y < 5) {
        printf("y is less than 5\n");
    }
    
    return 0;
}