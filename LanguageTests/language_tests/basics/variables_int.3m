#include <stdio.tm>

int main() {
    int a = 10;
    int b = -5;
    int c = 0;
    
    printf("a = %d\n", a);
    printf("b = %d\n", b);
    printf("c = %d\n", c);
    
    int sum = a + b;
    int product = a * b;
    int difference = a - b;
    
    printf("sum = %d\n", sum);
    printf("product = %d\n", product);
    printf("difference = %d\n", difference);
    
    a = 100;
    printf("a after assignment = %d\n", a);
    
    return 0;
}