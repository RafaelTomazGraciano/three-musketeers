#include <stdio.tm>

int main() {
    string str1 = "Hello";
    string str2 = "World";
    string empty = "";
    
    puts(str1);
    puts(str2);
    printf("empty string: '%s'\n", empty);
    
    printf("%s %s\n", str1, str2);
    
    str1 = "Modified";
    puts(str1);
    
    string special = "Tab:\there\nNewline:here";
    puts(special);
    
    return 0;
}