#include <stdio.tm>

int main() {
    double x = 3.14;
    double y = -2.5;
    double z = 0.0;
    
    printf("x = %f\n", x);
    printf("y = %f\n", y);
    printf("z = %f\n", z);
    
    double sum = x + y;
    double product = x * y;
    double division = x / y;
    
    printf("sum = %f\n", sum);
    printf("product = %f\n", product);
    printf("division = %f\n", division);
    
    x = 9.99;
    printf("x after assignment = %f\n", x);
    
    return 0;
}