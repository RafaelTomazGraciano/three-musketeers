#include <stdio.tm>

struct Student {
    int id,
    int grade
};

int main() {
    struct Student students[3];
    int i;
    int sum;
    
    printf("=== Struct Array Test ===\n");
    
    students[0].id = 101;
    students[0].grade = 85;
    
    students[1].id = 102;
    students[1].grade = 92;
    
    students[2].id = 103;
    students[2].grade = 78;
    
    printf("Students:\n");
    for (i = 0; i < 3; i = i + 1) {
        printf("  ID: %d, Grade: %d\n", students[i].id, students[i].grade);
    }
    
    sum = 0;
    for (i = 0; i < 3; i = i + 1) {
        sum = sum + students[i].grade;
    }
    
    printf("Average grade: %d\n", sum / 3);
    
    return 0;
}