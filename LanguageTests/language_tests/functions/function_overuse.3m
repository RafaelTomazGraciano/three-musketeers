#include <stdio.tm>

int f1(int x) {
    return x + 1;
}

int f2(int x) {
    return f1(x) + 1;
}

int f3(int x) {
    return f2(x) + 1;
}

int f4(int x) {
    return f3(x) + 1;
}

int f5(int x) {
    return f4(x) + 1;
}

int deepNest(int x) {
    return f1(f2(f3(f4(f5(x)))));
}

int main() {
    int result;
    
    printf("Testing deeply nested calls:\n");
    
    result = f1(0);
    printf("f1(0) = %d\n", result);
    
    result = f2(0);
    printf("f2(0) = %d\n", result);
    
    result = f3(0);
    printf("f3(0) = %d\n", result);
    
    result = f4(0);
    printf("f4(0) = %d\n", result);
    
    result = f5(0);
    printf("f5(0) = %d\n", result);
    
    result = deepNest(0);
    printf("deepNest(0) = %d\n", result);
    
    return 0;
}