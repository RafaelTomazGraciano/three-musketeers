#include <stdio.tm>

int factorial(int n) {
    if (n <= 1) {
        return 1;
    }
    return n * factorial(n - 1);
}

int main() {
    int i;
    
    printf("Factorial table:\n");
    for (i = 0; i <= 10; i = i + 1) {
        printf("factorial(%d) = %d\n", i, factorial(i));
    }
    
    return 0;
}