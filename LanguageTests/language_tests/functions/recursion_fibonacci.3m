#include <stdio.tm>

int fibonacci(int n) {
    if (n <= 1) {
        return n;
    }
    return fibonacci(n - 1) + fibonacci(n - 2);
}

int main() {
    int i;
    
    printf("Fibonacci sequence:\n");
    for (i = 0; i <= 10; i = i + 1) {
        printf("fibonacci(%d) = %d\n", i, fibonacci(i));
    }
    
    return 0;
}