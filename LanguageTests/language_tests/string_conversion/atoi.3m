#include <stdio.tm>
#include <stdlib.tm>

struct myStruct {
    string numStr,
    string anotherNumStr
};

union myUnion {
    string unionStr1,
    string unionStr2
};

int main() {
    string str1 = "123";
    string str2 = "-456";
    string str3 = "0";
    
    myStruct s;
    s.numStr = "789";
    s.anotherNumStr = "-999";
    
    myUnion u;
    u.unionStr1 = "-101";
    int num7 = atoi(u.unionStr1);
    
    u.unionStr2 = "202";
    int num8 = atoi(u.unionStr2);
    
    int num1 = atoi(str1);
    int num2 = atoi(str2);
    int num3 = atoi(str3);
    int num4 = atoi(s.numStr);
    int num5 = atoi(s.anotherNumStr);
    int num6 = atoi("42");
    
    printf("%d\n", num1);
    printf("%d\n", num2);
    printf("%d\n", num3);
    printf("%d\n", num4);
    printf("%d\n", num5);
    printf("%d\n", num6);
    printf("%d\n", num7); 
    printf("%d\n", num8);
    
    return 0;
}