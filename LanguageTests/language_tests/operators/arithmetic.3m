#include <stdio.tm>

int main() {
    int a = 20;
    int b = 7;
    
    printf("a = %d, b = %d\n", a, b);
    printf("a + b = %d\n", a + b);
    printf("a - b = %d\n", a - b);
    printf("a * b = %d\n", a * b);
    printf("a / b = %d\n", a / b);
    printf("a %% b = %d\n", a % b);
    
    double x = 15.0;
    double y = 4.0;
    
    printf("x = %f, y = %f\n", x, y);
    printf("x + y = %f\n", x + y);
    printf("x - y = %f\n", x - y);
    printf("x * y = %f\n", x * y);
    printf("x / y = %f\n", x / y);
    
    int zero = 0;
    printf("zero + 10 = %d\n", zero + 10);
    printf("zero * 10 = %d\n", zero * 10);
    
    int negative = -5;
    printf("negative + 15 = %d\n", negative + 15);
    printf("negative * 3 = %d\n", negative * 3);
    
    return 0;
}