#include <stdio.tm>

int main() {
    int a = 10;
    printf("a = %d\n", a);
    
    a = 20;
    printf("after a = 20: %d\n", a);
    
    a += 5;
    printf("after a += 5: %d\n", a);
    
    a -= 8;
    printf("after a -= 8: %d\n", a);
    
    a *= 2;
    printf("after a *= 2: %d\n", a);
    
    a /= 3;
    printf("after a /= 3: %d\n", a);
    
    double x = 10.0;
    printf("x = %f\n", x);
    
    x += 2.5;
    printf("after x += 2.5: %f\n", x);
    
    x -= 1.5;
    printf("after x -= 1.5: %f\n", x);
    
    x *= 2.0;
    printf("after x *= 2.0: %f\n", x);
    
    x /= 4.0;
    printf("after x /= 4.0: %f\n", x);
    
    return 0;
}