#include <stdio.tm>

int main(){
    int x = 10;
    double y = 5.5;
    bool z = true;
    char c = 'a';
    string msg = "fim";

    printf("int x = %d\n", x);
    printf("double y = %.1f\n", y);
    printf("bool z = %d\n", z);
    printf("char c = %c\n", c);
    printf("Resultado: %s", msg);

    return 0;
}